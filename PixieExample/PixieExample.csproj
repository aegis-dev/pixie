  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\test.png" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Pixie\Pixie.csproj" />
  </ItemGroup>

  <ItemGroup>
