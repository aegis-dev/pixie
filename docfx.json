{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Pixie/**.csproj"
          ]
        }
      ],
      "dest": "docfx/api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docfx/api/**.yml",
          "docfx/api/index.md"
        ]
      },
      {
        "files": [
          "docfx/examples/**.md",
          "docfx/examples/**/toc.yml",
          "docfx/toc.yml",
          "docfx/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "docfx/images/**"
        ]
      }
    ],
    "output": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}